library(ggplot2)
# 定义函数
sigmoid <- function(x) x**2-5*x+2
# 创建数据点
x<-seq(-10, 15, by=0.01)
y<-sigmoid(x)
df<-data.frame(x, y)
# 用ggplot2来画图
g <- ggplot(df, aes(x,y))
g <- g + geom_line(col='red')
g <- g + geom_hline(yintercept = 0.5) + geom_vline(xintercept = 0) #坐标轴
g <- g + ggtitle("sigmoid")
g
