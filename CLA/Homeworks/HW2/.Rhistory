f <- function(x) {
return(sinh(x) - cosh(x) + 0.1)
}
my.bisect <- function(f, a, b, tol=1e-14) {
if (f(a)*f(b) > 0) {
stop("Wrong Guess")
}
guess <- (a+b) / 2
while (abs(f(guess)) > tol) {
if (f(a)*f(guess) < 0) {
b <- guess
}
else {
a <- guess
}
guess <- (a+b) / 2
}
return(guess)
}
print(my.bisect(f, 2, 3))
